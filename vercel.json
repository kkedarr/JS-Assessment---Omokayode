{
    "version": 2,
    "builds": [
        {
            "src": "project_name/assessment.js",
            "use": "@vercel/javascript",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        },
        {
            "src": "build.sh",
            "use":"@vercel/static-build",
            "config":{
                "distDir": "staticfiles"
            }
        }
    ],
    "routes": [
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "project_name/assessment.js"
        }
    ]
}